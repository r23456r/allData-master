        <result column="method_params" property="methodParams" />
        <result column="cron_expression" property="cronExpression" />
    </resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        id,
        status,
        create_by,
        create_time,
        create_dept,
        update_by,
        update_time,
        remark,
        job_name, bean_name, method_name, method_params, cron_expression
